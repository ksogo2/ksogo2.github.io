//ポリゴンを表示するための緯度経度を取得
  var path_ew, path_s, path_n,= [];
  $.getJSON("data/LatLngLine.json", function(json) {
      $(json.ew_lines).each(function() {
        path_ew.push(new google.maps.LatLng(Number(this.latitude), Number(this.longitude)));
      }
      $(json.s_lines).each(function() {
        path_s.push(new google.maps.LatLng(Number(this.latitude), Number(this.longitude)));
      }
      $(json.n_lines).each(function() {
        path_n.push(new google.maps.LatLng(Number(this.latitude), Number(this.longitude)));
      }
  });
  